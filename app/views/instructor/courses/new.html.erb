<div class = 'booyah-box col-xs-10 col-xs-offset-1 col-md-6 col-md-offset-3'>
  <h1>Add a new course</h1>
  <%= simple_form_for @course, :url => instructor_courses_path do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :cost %>
    <%= f.input :image %>
    <br />
   <%= f.submit "Create", class: 'btn-success pull-right' %>
  <% end %>
  <br />
</div>
